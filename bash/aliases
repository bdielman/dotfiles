# MacVim
alias e="mvim -v"

# Directory Stuff
alias ..="cd .."
alias cdd="cd -"
alias ...="cd ../.."
alias ls="ls -lG"
alias ll="ls -alh"
alias lsa="ls -lGa"

# MySQL
alias sqlstart="mysql.server start"
alias sqlstop="mysql.server stop"

# Rails
alias ss="script/server"
alias sc="bundle exec rails c"
alias cons="script/console"
alias db="script/dbconsole"
alias spr="script/passenger_restarter"
alias bails="bundle exec rails s"

# Shortcuts
alias dt="cd ~/desktop"
alias www="cd ~/Sites"
alias w3="cd ~/Sites/within3/bigred"
alias cc="cd ~/Sites/CivicCommons"
alias rb="cd ~/Sites/rtb-site"
alias ux="cd ~/Sites/within3/ux/"
